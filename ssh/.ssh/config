    HostName ssh.iwismer.ca
    User iwismer
    port 222
    compression yes
    RequestTTY yes
    RemoteCommand if ! screen -ls; then screen -dmiS term -h 15000; screen -drS term -X stuff $'cat /var/run/motd.dynamic\n'; fi; screen -dRR
